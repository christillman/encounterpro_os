			<xs:element name="UnitsBilled" type="xs:long" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The number of units actually billed to the payer</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ChargeBilled" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The actual fee billed to the payer</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:decimal">
							<xs:attribute name="Currency" type="xs:string" use="optional"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="UnitsRecovered" type="xs:long" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The number of units actually paid for</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ChargeRecovered" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The amount of money actually collected for this charge</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:decimal">
							<xs:attribute name="Currency" type="xs:string" use="optional"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Assessment" type="EncounterAssessmentType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EncounterAssessmentType">
		<xs:sequence>
			<xs:element name="Assessment">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="AssessmentID">
							<xs:attribute name="AssessmentID" type="AssessmentID" use="optional"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="ICD9Code"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="JMJDocumentContext">
		<xs:sequence>
			<xs:element name="ContextObject">
				<xs:annotation>
					<xs:documentation>General, Patient, Encounter, Assessment, Treatment, Observation, Attachment</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice minOccurs="0">
				<xs:element name="EncounterID" type="EncounterID"/>
				<xs:element name="AssessmentID"/>
				<xs:element name="TreatmentID"/>
			</xs:choice>
			<xs:element name="Purpose" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The reason this document is being sent.  If exists, it must be one of the values allowed by EpIE.  Purpose can affect routing through EpIE and workflow in EncounterPRO.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="40"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Message" type="MessageType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>If this document contains a message and no patient context then the message goes here</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AuthorityType">
		<xs:sequence>
			<xs:element name="StartDate" type="xs:date" minOccurs="0"/>
			<xs:element name="EndDate" type="xs:date" minOccurs="0"/>
			<xs:element name="Sequence" type="xs:string" minOccurs="0"/>
			<xs:element name="Authority" type="ActorID"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PatientRelationType">
		<xs:sequence>
			<xs:element name="RelationActor" type="ActorID"/>
			<xs:element name="Relationship" maxOccurs="unbounded">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="MaternalSibiling"/>
						<xs:enumeration value="PaternalSibling"/>
						<xs:enumeration value="Parent"/>
						<xs:enumeration value="Guardian"/>
						<xs:enumeration value="Guarantor"/>
						<xs:enumeration value="Payor"/>
						<xs:enumeration value="PrimaryDecisionMaker"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="RelationshipDescription" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TreatmentOrderType">
		<xs:sequence>
			<xs:element name="OrderType">
				<xs:annotation>
					<xs:documentation>Specifies type of order event.  Values include "PMH" for Past Medical History, "Order" for EHR Provider Order, "Refill" for medication refill order.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Order"/>
						<xs:enumeration value="PMH"/>
						<xs:enumeration value="Refill"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="OrderedBy" type="ActorID">
				<xs:annotation>
					<xs:documentation>Specifies actor id for provider that initiated this order event</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OrderedDateTime" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>Specifies date/time that this order event was initiated</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Encounter" type="EncounterID">
				<xs:annotation>
					<xs:documentation>Specifies encounter id for encounter in which this order event was initiated.  May be missing for PMH orders</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Description" type="xs:string">
				<xs:annotation>
					<xs:documentation>Description of this order event.  If no order-specific description then this will contain the treatment description.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
