	<xs:element name="JMJDocument">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="CustomerID" type="xs:string"/>
				<xs:element name="OwnerID" type="xs:string"/>
				<xs:element name="Creator">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="CreateDate">
								<xs:annotation>
									<xs:documentation>Date and time this document was created</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="VendorName">
								<xs:annotation>
									<xs:documentation>Name of vendor who's application created this document (e.g. EHR, Inc.)</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ApplicationName">
								<xs:annotation>
									<xs:documentation>Name of application that ordered or created this document (e.g. EncounterPRO)</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ApplicationVersion">
								<xs:annotation>
									<xs:documentation>Version of application that ordered or created this document (e.g. 5.0.28)</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ComponentName" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Name of component that created this document (e.g. JMJDocument)</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ComponentVersion" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Version of component that created this document (e.g. 1.0.4.2) </xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="DatabaseModLevel" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Database Mod Level at the time this document was created</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="OfficeID" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Office ID where the task that created or ordered the creation of this document is running</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ComputerID" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Computer ID where the process that created this document is running</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="TaskID" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Identifies the workflow task that created or ordered the creation of this document.  (I.E. p_Patient_wp_Item.id)</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Actors" type="Actors"/>
				<xs:element name="MessageID" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="SenderMessageID" minOccurs="0"/>
							<xs:element name="EpIEMessageID" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="EpIEHandler" type="EpIEHandlerType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Internal Use Only.  EpIEHandler allows EpIE to send handling instructions to EncounterPRO.  This is only needed when the EpIE transform cannot resolve an ambiguity and is rarely used.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="PrimaryContext" type="JMJDocumentContext" minOccurs="0">
					<xs:annotation>
						<xs:documentation>The Primary Context block identifies which context object under PatientRecord is the main focus of this document.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="PatientRecord" type="PatientRecord"/>
			</xs:sequence>
			<xs:attribute name="Description" type="xs:string" use="optional"/>
			<xs:attribute name="DocumentID" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="PatientRecord">
		<xs:sequence>
			<xs:element name="PatientID" type="PatientID" maxOccurs="unbounded"/>
			<xs:element name="PatientHandling" type="ObjectHandling" minOccurs="0"/>
			<xs:element name="race" type="xs:string" minOccurs="0"/>
			<xs:element name="DateOfBirth" type="xs:date" minOccurs="0"/>
			<xs:element name="TimeOfBirth" type="xs:time" minOccurs="0"/>
			<xs:element name="sex" type="xs:string" minOccurs="0"/>
			<xs:element name="PrimaryLanguage" type="xs:string" minOccurs="0"/>
			<xs:element name="MaritalStatus" type="xs:string" minOccurs="0"/>
			<xs:element name="SSN" type="xs:string" minOccurs="0"/>
			<xs:element name="FirstName" type="xs:string"/>
			<xs:element name="LastName" type="xs:string"/>
			<xs:element name="Degree" type="xs:string" minOccurs="0"/>
			<xs:element name="NamePrefix" type="xs:string" minOccurs="0"/>
			<xs:element name="MiddleName" type="xs:string" minOccurs="0"/>
			<xs:element name="NameSuffix" type="xs:string" minOccurs="0"/>
			<xs:element name="MaidenName" type="xs:string" minOccurs="0"/>
			<xs:element name="Communication" type="CommunicationType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="PrimaryLocation" type="ActorID" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Physician's office location where patient is managed and/or usually comes for office visits.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PatientStatus" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Active, Inactive, Dismissed, Deceased, Non-Patient Data, Relation</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PatientAddress" type="AddressType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Religion" type="xs:string" minOccurs="0"/>
			<xs:element name="Nationality" type="xs:string" minOccurs="0"/>
			<xs:element name="FinancialClass" type="xs:string" minOccurs="0"/>
			<xs:element name="Employer" type="xs:string" minOccurs="0"/>
			<xs:element name="EmployeeID" type="xs:string" minOccurs="0"/>
			<xs:element name="Department" type="xs:string" minOccurs="0"/>
			<xs:element name="Shift" type="xs:string" minOccurs="0"/>
			<xs:element name="JobDescription" type="xs:string" minOccurs="0"/>
			<xs:element name="StartDate" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="TerminationDate" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="EmploymentStatus" type="xs:string" minOccurs="0"/>
			<xs:element name="Relation" type="PatientRelationType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="PrimaryProvider" type="ActorID" minOccurs="0"/>
			<xs:element name="SecondaryProvider" type="ActorID" minOccurs="0"/>
			<xs:element name="ReferringProvider" type="ActorID" minOccurs="0"/>
			<xs:element name="Authority" type="AuthorityType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>List of authority actors that have a relationship with this patien (e.g. Payers, P4P)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Encounter" type="Encounter" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Assessment" type="Assessment" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Treatment" type="Treatment" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="PatientNote" type="ChartNoteType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Message" type="MessageType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Encounter">
		<xs:sequence>
			<xs:element name="ObjectID" type="ObjectID" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="EncounterHandling" type="ObjectHandling" minOccurs="0"/>
			<xs:element name="EncounterDate" type="xs:dateTime"/>
			<xs:element name="Description" type="xs:string"/>
			<xs:element name="EncounterType" minOccurs="0">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="Description" use="optional">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="80"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="AttendingDoctor" type="ActorID" minOccurs="0"/>
			<xs:element name="ReferringDoctor" type="ActorID" minOccurs="0"/>
			<xs:element name="SupervisingDoctor" type="ActorID" minOccurs="0"/>
			<xs:element name="NewPatient" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="New"/>
						<xs:enumeration value="Established"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="EncounterLocation" type="ActorID" minOccurs="0"/>
			<xs:element name="WorkersCompFlag" type="xs:string" minOccurs="0"/>
			<xs:choice minOccurs="0">
				<xs:element name="InpatientStatus" type="InpatientStatusType">
					<xs:annotation>
						<xs:documentation>An inpatient encounter represents an admission to a hospital or other inpatient facility</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="OutpatientStatus" type="OutpatientStatusType">
					<xs:annotation>
						<xs:documentation>An outpatient encounter is where the provider has direct contact with the patient but the encounter has a relatively finite duration</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="IndirectEncounterStatus" type="IndirectEncounterStatusType">
					<xs:annotation>
						<xs:documentation>An indirect encounter is where some form of communication took place but there was no direct contact between provider and patient</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="OtherEncounterStatus" type="OtherEncounterStatusType">
					<xs:annotation>
						<xs:documentation>An Other encouter is any process which updates the clinical record but does not fall into any of the other encounter modalities</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:element name="EncounterNote" type="ChartNoteType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Message" type="MessageType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Charge" type="EncounterChargeType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="EncounterID" type="EncounterID" use="optional"/>
	</xs:complexType>
	<xs:complexType name="Assessment">
		<xs:sequence>
			<xs:element name="ObjectID" type="ObjectID" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="AssessmentHandling" type="ObjectHandling" minOccurs="0"/>
			<xs:element name="AssessmentDefinitionID" type="xs:string" minOccurs="0"/>
			<xs:element name="OpenEncounter" type="EncounterID" minOccurs="0"/>
			<xs:element name="AssessmentType" type="xs:string"/>
			<xs:element name="Description" type="xs:string"/>
			<xs:element name="Location" type="xs:string" minOccurs="0"/>
			<xs:element name="Acuteness" type="xs:string" minOccurs="0"/>
			<xs:element name="ICD9" minOccurs="0"/>
			<xs:element name="BeginDate" type="xs:dateTime"/>
			<xs:element name="DiagnosedBy" type="ActorID" minOccurs="0"/>
			<xs:element name="AssessmentStatus" type="xs:string" minOccurs="0"/>
			<xs:element name="CloseEncounter" type="EncounterID" minOccurs="0"/>
			<xs:element name="EndDate" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="Treatment" type="TreatmentID" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="AssessmentNote" type="ChartNoteType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Message" type="MessageType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="AssessmentID" type="AssessmentID" use="optional"/>
	</xs:complexType>
	<xs:complexType name="Treatment">
		<xs:sequence>
			<xs:element name="ObjectID" type="ObjectID" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="TreatmentHandling" type="ObjectHandling" minOccurs="0"/>
			<xs:element name="OpenEncounter" type="EncounterID" minOccurs="0"/>
			<xs:element name="TreatmentType" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="24"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="BeginDate" type="xs:dateTime"/>
			<xs:element name="Description" type="xs:string"/>
			<xs:element name="SpecimenID" type="xs:string" minOccurs="0"/>
			<xs:element name="TreatmentLocation" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="40"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="TreatmentStatus" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="12"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="CloseEncounter" type="EncounterID" minOccurs="0"/>
			<xs:element name="EndDate" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="OrderedBy" type="ActorID" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The provider who originally ordered this treatment</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OrderedFor" type="ActorID" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contains the actor that the original ordering provider intended to perform this treatment.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Order" type="TreatmentOrderType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Contains a child block for each Order Event.  E.g. for medications, the original order and each refill is a seperate Order Event.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LastOrderedBy" type="ActorID" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The OrderedBy provider who issued the latest Order event.  This is a duplicate of the OrderedBy element from the last Order block.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LastOrderedDateTime" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The date/time of the latest Order event.  This is a duplicate of the OrderedDateTime element from the last Order block.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LastOrderedEncounter" type="EncounterID" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The Encounter associated with the latest Order event.  This is a duplicate of the Encounter element from the last Order block.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CompletedBy" type="ActorID" minOccurs="0"/>
			<xs:element name="Assessment" type="AssessmentID" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Observation" type="Observation" minOccurs="0"/>
			<xs:element name="TreatmentNote" type="ChartNoteType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:choice minOccurs="0">
				<xs:element name="Medication" type="TreatmentMedication"/>
			</xs:choice>
			<xs:element name="Message" type="MessageType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="ConstituentTreatments" minOccurs="0">
				<xs:annotation>
					<xs:documentation>If the parent treatment contains individual sub-elements, such as different vials withing an immunotherapy treatment, the sub-elements are modeled as Constituent Treatments (also called "Child Treatments")</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Treatment" type="Treatment" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="TreatmentID" type="TreatmentID" use="optional"/>
	</xs:complexType>
	<xs:complexType name="Observation">
		<xs:sequence>
			<xs:element name="ObjectID" type="ObjectID" maxOccurs="unbounded"/>
			<xs:element name="Description" type="xs:string"/>
			<xs:element name="Encounter" type="Encounter" minOccurs="0"/>
			<xs:element name="ResultExpectedDate" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="Stage" type="xs:int" minOccurs="0"/>
			<xs:element name="StageDescription" type="xs:string" minOccurs="0"/>
			<xs:element name="ObservationResult" type="ObservationResult" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Observation" type="Observation" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="ObservationNote" type="ChartNoteType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>ObservationNote is preserved for backward compatibility but is depricated and may not be supported in a future release</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SpecimenCollected" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="ObservationStatus" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="12"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Attachment">
		<xs:sequence>
			<xs:element name="ID" type="ObjectID"/>
			<xs:element name="AttachmentType" type="xs:string"/>
			<xs:element name="Description" type="xs:string"/>
			<xs:element name="Extension" type="xs:string"/>
			<xs:element name="TextVersion" type="xs:string"/>
			<xs:element name="Data" type="xs:hexBinary"/>
			<xs:element name="AttachmentDate" type="xs:dateTime"/>
			<xs:element name="AttachedBy" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ObjectID">
		<xs:sequence>
			<xs:element name="OwnerID" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Specifies owner of IDDomain and IDValue.  If not supplied then taken from JMJDocument/OwnerID</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IDDomain" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Specifies the domain of possible values for IDValue</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IDValue" type="xs:string" minOccurs="0"/>
			<xs:element name="CustomerID" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Specifies owner of JMJDomain and JMJValue.  If not supplied then taken from JMJDocument/CustomerID</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="JMJDomain" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Specifies the domain within the EncounterPRO database of the values for JMJValue.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="JMJValue" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Specified the actual identifier or key with the EncounterPRO database</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ObjectHandling">
		<xs:sequence>
			<xs:element name="Purpose" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Instructs the receiving system or EpIE of the purpose of this object block.  E.g. New Rx, Cancel Lab Order, etc.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ObjectUpdate" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Instructs the receiving system on how to handle the object if it is found to already exist.  Default is UpdateNever.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="UpdateAlways"/>
						<xs:enumeration value="UpdateNever"/>
						<xs:enumeration value="UpdateAsk"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ObjectCreate" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Instructs the receiving system on how to handle this object if it can't find it in the database.  Default is CreateNever.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="CreateNever"/>
						<xs:enumeration value="CreateAlways"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Notify" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Identifies provider who should be notified of the arrival of the new data.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="OrderingProvider"/>
						<xs:enumeration value="PrimaryProvider"/>
						<xs:enumeration value="EncounterProvider"/>
						<xs:enumeration value="NoOne"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="NewObjectWorkflowName" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Used to lookup specific workplan to order if new object is created from this data.  (i.e. Treatment Mode for treatments).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ExistingObjectWorkflowName" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Used to lookup specific workplan to order if existing object is found from this data.  (i.e. Treatment Mode for treatments).</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PatientID">
		<xs:sequence>
			<xs:element name="OwnerID" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Identifies owner of this IDDomain.  If not supplied then if PatientIDDomain begins with JMJ the OwnerID defaults to the CustomerID.  Otherwise it defaults to the OwnerID of thsi document.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PatientIDDomain">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="40"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="PatientID">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="80"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ObservationResult">
		<xs:sequence>
			<xs:element name="ObjectID" type="ObjectID" maxOccurs="unbounded"/>
			<xs:element name="location" type="xs:string" minOccurs="0"/>
			<xs:element name="result_definition_id" type="xs:string" minOccurs="0"/>
			<xs:element name="encounter_date" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="ResultDate" type="xs:dateTime"/>
			<xs:element name="ResultType" type="xs:string"/>
