 retrieve="SELECT r.maintenance_rule_id,
	r.description,
	r.last_reset ,
	r.patients ,
	r.compliant_patients ,
	r.measured_patients ,
	r.controlled_patients ,
     compliance_ok_percent = COALESCE(c.compliance_ok_percent, 90),
     compliance_warning_percent = COALESCE(c.compliance_warning_percent, 80),
     measured_ok_percent = COALESCE(c.measured_ok_percent, 90),
     measured_warning_percent = COALESCE(c.measured_warning_percent, 80),
     controlled_ok_percent = COALESCE(c.controlled_ok_percent, 90),
     controlled_warning_percent = COALESCE(c.controlled_warning_percent, 80),
	c.status,
	selected_flag=0
FROM dbo.fn_hm_classes(:maintenance_rule_type) r
	INNER JOIN c_Maintenance_Patient_Class c
	ON r.maintenance_rule_id = c.maintenance_rule_id

" arguments=(("maintenance_rule_type", string))  sort="description A maintenance_rule_id A " )
