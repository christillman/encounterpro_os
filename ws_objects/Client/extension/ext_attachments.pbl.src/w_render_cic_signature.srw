on w_render_cic_signature.create
int iCurrent
call super::create
this.pb_done=create pb_done
this.st_title=create st_title
this.ole_cic_signature=create ole_cic_signature
iCurrent=UpperBound(this.Control)
this.Control[iCurrent+1]=this.pb_done
this.Control[iCurrent+2]=this.st_title
this.Control[iCurrent+3]=this.ole_cic_signature
end on

on w_render_cic_signature.destroy
call super::destroy
destroy(this.pb_done)
destroy(this.st_title)
destroy(this.ole_cic_signature)
end on

event open;call super::open;str_popup popup

popup = message.powerobjectparm
If popup.data_row_count = 4 then
	ink = popup.items[1]
	bitmap_file = popup.items[2]
	bitmap_width = long(popup.items[3])
	bitmap_height = long(popup.items[4])
	
	if isnull(ink) then
		pb_done.event clicked()
		Return
	end if		
	
	postevent("post_open")
Else
	log.log(this,"w_render_cic_signature:open","Invalid parameters",4)
	pb_done.event clicked()
	Return
End If
end event

event post_open;long ll_sts
boolean lb_size
blob lbl_attachment

if isnull(bitmap_height) or bitmap_height <= 0 or isnull(bitmap_width) or bitmap_width <= 0 then
	lb_size = false
else
	lb_size = true
end if

ole_cic_signature.object.ink = ink

if ole_cic_signature.object.ink = ink then
	if lb_size then
		ll_sts = ole_cic_signature.object.SaveToBitmapFileEx(bitmap_width, bitmap_height, bitmap_file)
	else
		ll_sts = ole_cic_signature.object.SaveToBitmapFile(bitmap_file)
	end if
else
	log.log(this, "w_render_cic_signature:post", "Invalid Ink Property", 3)
	ll_sts = -1
	setnull(bitmap_file)
end if
pb_done.event clicked()
end event

