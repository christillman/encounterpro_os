global function date f_add_years (date pd_date, integer pi_years);integer li_year, li_month, li_day

li_year = year(pd_date)
li_month = month(pd_date)
li_day = day(pd_date)

li_year += pi_years

return date(li_year, li_month, li_day)

end function

