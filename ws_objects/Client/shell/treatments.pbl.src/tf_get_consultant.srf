global function integer tf_get_consultant (string ps_consultant_id, ref string ps_description);

tf_begin_transaction(this, "")

  SELECT description
    INTO :ps_description
    FROM c_Consultant
   WHERE consultant_id = :ps_consultant_id;

if not tf_check() then return -1

tf_commit()

return 1
end function

