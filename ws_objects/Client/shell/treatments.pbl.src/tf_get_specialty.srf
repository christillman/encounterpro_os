global function integer tf_get_specialty (string ps_specialty_id, ref string ps_description);

tf_begin_transaction(this, "")

  SELECT description
    INTO :ps_description
    FROM c_Specialty
   WHERE specialty_id = :ps_specialty_id;

if not tf_check() then return -1

tf_commit()

return 1
end function

