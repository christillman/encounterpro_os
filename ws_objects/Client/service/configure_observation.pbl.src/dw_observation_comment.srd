 retrieve="  SELECT cpr_id,   
         observation_sequence,   
         Observation_comment_id,   
         observation_id,   
         comment_title,   
         abnormal_flag,   
         severity,   
         treatment_id,   
         encounter_id,   
         attachment_id,   
         user_id,   
         created_by,   
         created,   
		 CASE WHEN short_comment Is Null THEN comment ELSE short_comment END AS comment,   
         comment_date_time,   
         comment_type,   
         root_observation_sequence  
    FROM p_Observation_Comment  
   WHERE ( cpr_id = :cpr_id ) AND  
         ( observation_sequence = :observation_sequence ) AND  
         ( comment_title like :comment_title ) AND  
         ( current_flag = 'Y' )    
" update="p_Observation_Comment" updatewhere=0 updatekeyinplace=no arguments=(("cpr_id", string),("observation_sequence", number),("comment_title", string))  sort="observation_comment_id A " )
